<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <ripple android:color="@color/primary" android:radius="24dp" />
    </item>
    <item
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/ic_baseline_arrow_forward_8"
        android:gravity="center" />
    <item
        android:width="44dp"
        android:height="44dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid
                android:color="@android:color/transparent" />
            <stroke
                android:width="1dp"
                android:color="@color/primary" />
        </shape>
    </item>
</layer-list>